{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "bakerkretzmar/nova-settings-tool": "^2.0",
    "bolechen/nova-activitylog": "^0.4.0",
    "codenco-dev/nova-grid-system": "^2.0",
    "coroowicaksono/chart-js-integration": "^0.4.0",
    "ctessier/nova-advanced-image-field": "^2.0",
    "dniccum/phone-number": "^2.2",
    "ebess/advanced-nova-media-library": "^4.1",
    "eminiarts/nova-tabs": "^2.2",
    "guzzlehttp/guzzle": "^7.2",
    "interaction-design-foundation/nova-html-card": "^3.1",
    "itsmejoshua/novaspatiepermissions": "^1.0",
    "jeffbeltran/sanctum-tokens": "^2.0",
    "laravel/framework": "^10.0",
    "laravel/nova": "~4.0",
    "laravel/pennant": "^1.2",
    "laravel/sanctum": "^3.2",
    "laravel/socialite": "^5.6",
    "laravel/telescope": "^4.14",
    "laravel/tinker": "^2.8",
    "llaski/nova-scheduled-jobs": "^6.1",
    "outl1ne/nova-sortable": "^3.4",
    "pdmfc/nova-action-button": "^0.7.4",
    "saintsystems/nova-linkable-metrics": "^4.1",
    "spatie/laravel-activitylog": "^4.7",
    "spatie/laravel-data": "^3.4",
    "spatie/laravel-enum": "^3.0",
    "spatie/laravel-flash": "^1.9",
    "spatie/laravel-permission": "^5.10",
    "spatie/laravel-query-builder": "^5.2",
    "spatie/laravel-schemaless-attributes": "^2.4",
    "spatie/laravel-settings": "^3.0",
    "spatie/laravel-sluggable": "^3.4",
    "spatie/laravel-tags": "^4.3",
    "spatie/laravel-translatable": "^6.5",
    "spatie/nova-backup-tool": "^5.0",
    "spatie/nova-tags-field": "^4.0",
    "vmitchell85/nova-links": "^2.1",
    "vyuldashev/nova-money-field": "^2.0",
    "vyuldashev/nova-permission": "^3.2",
    "whitecube/nova-flexible-content": "^1.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8",
    "doctrine/dbal": "^3.6",
    "fakerphp/faker": "^1.9.1",
    "laravel/dusk": "^7.7",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan optimize:clear",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan nova:publish",
      "@php artisan optimize:clear"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
